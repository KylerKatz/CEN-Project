{
	"name": "react_template_repo",
	"version": "1.0.0",
	"engines": {
		"node": "13.x"
	},
	"type": "module",
	"description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
	"main": "server/server.js",
	"scripts": {
		"test": "mocha server/tests",
		"install-all": "concurrently \"npm install\" \"cd client && npm install\"",
		"client": "cd client && npm start",
		"server": "nodemon server/server.js",
		"dev": "concurrently \"nodemon server/server.js\" \"cd client && npm run start\"",
		"build": "cd client && npm build",
		"noah": "cd client && cd build && npm run-script build",
		"start": "node server/server.js",
		"heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/KylerKatz/CEN-Project.git"
	},
	"keywords": [],
	"author": "Dakota Rennemann",
	"license": "MIT",
	"dependencies": {
		"all": "0.0.0",
		"async": "^3.2.0",
		"axios": "^0.19.2",
		"bcrypt": "^4.0.1",
		"body-parser": "^1.20.1",
		"concurrently": "^4.1.0",
		"connect-flash": "^0.1.1",
		"cookie-parser": "^1.4.5",
		"cors": "^2.8.5",
		"express": "^4.18.2",
		"express-session": "^1.17.0",
		"express-validator": "^5.3.1",
		"heroku": "^7.39.0",
		"mocha": "^6.2.1",
		"mongoose": "^5.7.1",
		"morgan": "^1.9.1",
		"nodemon": "^1.18.10",
		"passport": "^0.4.1",
		"passport-local": "^1.0.0",
		"should": "^13.2.3",
		"supertest": "^4.0.2"
	},
	"homepage": "https://github.com/KylerKatz/CEN-Project#readme"
}
